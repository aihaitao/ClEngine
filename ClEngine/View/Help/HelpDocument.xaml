<UserControl x:Class="ClEngine.HelpDocument"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:model="clr-namespace:ClEngine.Model"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             xmlns:help="clr-namespace:ClEngine.View.Help"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl TabStripPlacement="Left">
            <TabItem Header="system">
                <help:SystemDocumentView></help:SystemDocumentView>
            </TabItem>
            <TabItem Header="content"></TabItem>
            <TabItem Header="camera"></TabItem>
        </TabControl>
    </Grid>
</UserControl>
